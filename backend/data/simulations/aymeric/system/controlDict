FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pisoFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         10;

deltaT          0.1;

writeControl    timeStep;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

writeOverwrite false;


// ************************************************************************* //

Libs ("libforces.so");

functions
{
    forces
    {
        type forces;
        enabled true;

        patches (tube);
        rhoName none;
        rhoInf 1000;

        log true;
        writeControl timeStep;
writeInterval   1;

        CofR (0 0 0);
        dragDir (0 0 1);
        liftDir (0 1 0);
    }



}
