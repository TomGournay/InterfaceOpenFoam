FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 1);
    }

    outlet
    {
        type            zeroGradient;
    }

    wall
    {
        type            symmetry;
    }

    symmetry
    {
        type            symmetry;
    }

    tube
    {
        type            noSlip;
    }
}
